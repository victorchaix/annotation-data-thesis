#################################################################
#    Data properties
#################################################################

###  http://purl.org/ontology/TextualReknittings/containsText
<http://purl.org/ontology/TextualReknittings/containsText> rdf:type owl:DatatypeProperty ;
                                                           rdfs:domain <http://purl.org/ontology/TextualReknittings/TextAnnotations> ;
                                                           rdfs:range rdfs:Literal ;
                                                           rdfs:comment "The text that is included in an annotation." .


###  http://purl.org/ontology/TextualReknittings/hasAnnotations
<http://purl.org/ontology/TextualReknittings/hasAnnotations> rdf:type owl:DatatypeProperty ;
                                                             rdfs:domain <http://purl.org/ontology/TextualReknittings/Annotator> ;
                                                             rdfs:range xsd:integer ;
                                                             rdfs:comment "Number of annotations made by an annotator as part of a digital social reading experiment." .


###  http://purl.org/ontology/TextualReknittings/hasAuthor
<http://purl.org/ontology/TextualReknittings/hasAuthor> rdf:type owl:DatatypeProperty ;
                                                        rdfs:domain <http://purl.org/ontology/TextualReknittings/InitialTexts> ;
                                                        rdfs:range rdfs:Literal ;
                                                        rdfs:comment "The original author(s) of the initial text that has been chosen by the group." .


###  http://purl.org/ontology/TextualReknittings/hasSource
<http://purl.org/ontology/TextualReknittings/hasSource> rdf:type owl:DatatypeProperty ;
                                                        rdfs:domain <http://purl.org/ontology/TextualReknittings/InitialTexts> ;
                                                        rdfs:range rdfs:Literal ;
                                                        rdfs:comment "The URL of the web text that has been read." .


#################################################################
#    Classes
#################################################################

###  http://purl.org/ontology/TextualReknittings/AnnotationsReply
<http://purl.org/ontology/TextualReknittings/AnnotationsReply> rdf:type owl:Class ;
                                                               rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/TextTypes> ;
                                                               owl:disjointWith <http://purl.org/ontology/TextualReknittings/TextAnnotations> ;
                                                               rdfs:comment "AnnotationsReply referers to further comments made on an annotation, usually by an other annotator." ;
                                                               rdfs:seeAlso "https://networkcultures.org/reading-group/textual-reknittings/annotation-archive/all-annotations.html" .


###  http://purl.org/ontology/TextualReknittings/Annotator
<http://purl.org/ontology/TextualReknittings/Annotator> rdf:type owl:Class ;
                                                        rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/Individuals> ;
                                                        owl:disjointWith <http://purl.org/ontology/TextualReknittings/Categorizer> ,
                                                                         <http://purl.org/ontology/TextualReknittings/Curator> ;
                                                        rdfs:comment "Individual that has annotated or replied to annotations over texts of the digital social reading experiment." .


###  http://purl.org/ontology/TextualReknittings/Categorizer
<http://purl.org/ontology/TextualReknittings/Categorizer> rdf:type owl:Class ;
                                                          rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/Individuals> ;
                                                          rdfs:comment "The individual responsible for the further tagging of the rewritten output in a wiki format, so that fragments of the output can also serve as an organized and categorized archive for the group." .


###  http://purl.org/ontology/TextualReknittings/Curator
<http://purl.org/ontology/TextualReknittings/Curator> rdf:type owl:Class ;
                                                      rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/Individuals> ;
                                                      rdfs:comment "The individual that is responsible for coordinating the digital social reading experiment, by animating it and by specifying to the participants the different tags, practices and goals." .


###  http://purl.org/ontology/TextualReknittings/Group
<http://purl.org/ontology/TextualReknittings/Group> rdf:type owl:Class ;
                                                    rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/Individuator> ;
                                                    rdfs:comment "The \"we\" that is taking consistency as part of the simultaneous rewriting of the different annotations, made in the reading and annotating part." .


###  http://purl.org/ontology/TextualReknittings/Individuals
<http://purl.org/ontology/TextualReknittings/Individuals> rdf:type owl:Class ;
                                                          rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/Individuator> ;
                                                          rdfs:comment "Participants to the curation, annotation and further categorization of the output of a digital social reading experiment." .


###  http://purl.org/ontology/TextualReknittings/Individuator
<http://purl.org/ontology/TextualReknittings/Individuator> rdf:type owl:Class ;
                                                           rdfs:comment "Individuator consists of the human reader or writer which individuates a text - that is, adds developments to it through annotation and/or rewriting - and concomitantly to this process, individuates itself." ;
                                                           rdfs:seeAlso "https://arsindustrialis.org/individuation" .


###  http://purl.org/ontology/TextualReknittings/InitialTexts
<http://purl.org/ontology/TextualReknittings/InitialTexts> rdf:type owl:Class ;
                                                           rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/TextTypes> ;
                                                           owl:disjointWith <http://purl.org/ontology/TextualReknittings/RefragmentedWiki> ,
                                                                            <http://purl.org/ontology/TextualReknittings/Rewriting> ;
                                                           rdfs:comment "InitialTexts are the texts chosen by the group to read and annotate collectively, to produce a collective interpretation and appropriation of them for their own goals." .


###  http://purl.org/ontology/TextualReknittings/RefragmentedWiki
<http://purl.org/ontology/TextualReknittings/RefragmentedWiki> rdf:type owl:Class ;
                                                               rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/TextTypes> ;
                                                               rdfs:comment "RefragmentedWiki consists of the further tagging of the rewritten output, in a wiki webpage that separates the different parts of the output as well as the categories of each of its textual fragments. Navigation in the wiki is as such possible through either through the linear parts of the rewritten output or the non-linear tags that have been added to it." ;
                                                               rdfs:seeAlso "https://networkcultures.org/reading-group/textual-reknittings/non-linear-output/reports.html" .


###  http://purl.org/ontology/TextualReknittings/Rewriter
<http://purl.org/ontology/TextualReknittings/Rewriter> rdf:type owl:Class ;
                                                       rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/Group> ;
                                                       rdfs:comment "The rewriter is necessarily a group activity so that the digital social reading experiment becomes a digital social writing one, and for the produced digital social text to reflect a collective interpretation and appropriation of the initial texts." .


###  http://purl.org/ontology/TextualReknittings/Rewriting
<http://purl.org/ontology/TextualReknittings/Rewriting> rdf:type owl:Class ;
                                                        rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/TextTypes> ;
                                                        rdfs:comment "Rewriting refers to the creative juxtaposition of annotations and their replies, to produce a new text that is a collective interpretation and appropriation of the initial texts." ;
                                                        rdfs:seeAlso "https://networkcultures.org/reading-group/textual-reknittings/textual-reknittings.html" .


###  http://purl.org/ontology/TextualReknittings/Tags
<http://purl.org/ontology/TextualReknittings/Tags> rdf:type owl:Class ;
                                                   rdfs:comment "Tags are the meta-categories that have been specified and adopted collectively as part of a digital social reading experiment, to categorize each of the annotations and their replies. The tags taxonomy can also serve for a refragmented wiki of the rewriting output." .


###  http://purl.org/ontology/TextualReknittings/TextAnnotations
<http://purl.org/ontology/TextualReknittings/TextAnnotations> rdf:type owl:Class ;
                                                              rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/TextTypes> ;
                                                              rdfs:comment "TextAnnotations are selected fragments in the text that is read, that can be tagged and commented." ;
                                                              rdfs:seeAlso "https://networkcultures.org/longform/2023/07/13/hypothes-is-a-story-of-playing-with-digital-texts-expandable-confines/" ,
                                                                           "https://networkcultures.org/reading-group/textual-reknittings/annotation-archive/all-annotations.html" .


###  http://purl.org/ontology/TextualReknittings/TextTypes
<http://purl.org/ontology/TextualReknittings/TextTypes> rdf:type owl:Class ;
                                                        rdfs:comment "TextTypes refers to the different state of the text in the process of the 'Textual Reknittings' workshop, from the initial state of chosen texts on the web, to annotations or annotations reply on them, to the rewriting of the annotations and selected fragments, to the re-fragmentation of the rewriting output in a Wiki form through re-tagging." ;
                                                        rdfs:seeAlso "https://hackersanddesigners.nl/s/Summer_Camp_2023/p/Textual_Re-knittings" .


###  http://purl.org/ontology/TextualReknittings/affection
<http://purl.org/ontology/TextualReknittings/affection> rdf:type owl:Class ;
                                                        rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/Tags> ;
                                                        owl:disjointWith <http://purl.org/ontology/TextualReknittings/approval> ,
                                                                         <http://purl.org/ontology/TextualReknittings/citation> ,
                                                                         <http://purl.org/ontology/TextualReknittings/commentary> ,
                                                                         <http://purl.org/ontology/TextualReknittings/disapproval> ,
                                                                         <http://purl.org/ontology/TextualReknittings/important> ,
                                                                         <http://purl.org/ontology/TextualReknittings/key-word> ,
                                                                         <http://purl.org/ontology/TextualReknittings/trouble> ;
                                                        rdfs:comment "What is appreciated, well formulated, touches the emotions." .


###  http://purl.org/ontology/TextualReknittings/approval
<http://purl.org/ontology/TextualReknittings/approval> rdf:type owl:Class ;
                                                       rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/Tags> ;
                                                       owl:disjointWith <http://purl.org/ontology/TextualReknittings/citation> ,
                                                                        <http://purl.org/ontology/TextualReknittings/commentary> ,
                                                                        <http://purl.org/ontology/TextualReknittings/disapproval> ,
                                                                        <http://purl.org/ontology/TextualReknittings/important> ,
                                                                        <http://purl.org/ontology/TextualReknittings/key-word> ,
                                                                        <http://purl.org/ontology/TextualReknittings/trouble> ;
                                                       rdfs:comment "What is agreed upon, fits into preconceptions." .


###  http://purl.org/ontology/TextualReknittings/citation
<http://purl.org/ontology/TextualReknittings/citation> rdf:type owl:Class ;
                                                       rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/Tags> ;
                                                       owl:disjointWith <http://purl.org/ontology/TextualReknittings/commentary> ,
                                                                        <http://purl.org/ontology/TextualReknittings/disapproval> ,
                                                                        <http://purl.org/ontology/TextualReknittings/important> ,
                                                                        <http://purl.org/ontology/TextualReknittings/key-word> ,
                                                                        <http://purl.org/ontology/TextualReknittings/trouble> ;
                                                       rdfs:comment "What strikes out as important external material/reference, supports an idea." .


###  http://purl.org/ontology/TextualReknittings/commentary
<http://purl.org/ontology/TextualReknittings/commentary> rdf:type owl:Class ;
                                                         rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/Tags> ;
                                                         owl:disjointWith <http://purl.org/ontology/TextualReknittings/disapproval> ,
                                                                          <http://purl.org/ontology/TextualReknittings/important> ,
                                                                          <http://purl.org/ontology/TextualReknittings/key-word> ,
                                                                          <http://purl.org/ontology/TextualReknittings/trouble> ;
                                                         rdfs:comment "wWhat is missing from the textual information and should be added." .


###  http://purl.org/ontology/TextualReknittings/disapproval
<http://purl.org/ontology/TextualReknittings/disapproval> rdf:type owl:Class ;
                                                          rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/Tags> ;
                                                          owl:disjointWith <http://purl.org/ontology/TextualReknittings/important> ,
                                                                           <http://purl.org/ontology/TextualReknittings/key-word> ,
                                                                           <http://purl.org/ontology/TextualReknittings/trouble> ;
                                                          rdfs:comment "What is disagreed upon, does not fit into preconceptions." .


###  http://purl.org/ontology/TextualReknittings/important
<http://purl.org/ontology/TextualReknittings/important> rdf:type owl:Class ;
                                                        rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/Tags> ;
                                                        owl:disjointWith <http://purl.org/ontology/TextualReknittings/key-word> ,
                                                                         <http://purl.org/ontology/TextualReknittings/trouble> ;
                                                        rdfs:comment "What supports." .


###  http://purl.org/ontology/TextualReknittings/key-word
<http://purl.org/ontology/TextualReknittings/key-word> rdf:type owl:Class ;
                                                       rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/Tags> ;
                                                       owl:disjointWith <http://purl.org/ontology/TextualReknittings/trouble> ;
                                                       rdfs:comment "What strikes out as key notions in a text, central to an idea/argument." .


###  http://purl.org/ontology/TextualReknittings/trouble
<http://purl.org/ontology/TextualReknittings/trouble> rdf:type owl:Class ;
                                                      rdfs:subClassOf <http://purl.org/ontology/TextualReknittings/Tags> ;
                                                      rdfs:comment "What suprises." .


